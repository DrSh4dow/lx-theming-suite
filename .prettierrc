{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"importOrder": [
		"^(react/(.*)$)|^(react$)|^(react-native(.*)$)",
		"^(next/(.*)$)|^(next$)",
		"^(expo(.*)$)|^(expo$)",
		"<THIRD_PARTY_MODULES>",
		"",
		"^@acme/(.*)$",
		"^@freethefreela/(.*)$",
		"^$lib/(.*)$",
		"",
		"^~/",
		"^[../]",
		"^[./]"
	],
	"plugins": [
		"prettier-plugin-svelte",
		"@ianvs/prettier-plugin-sort-imports",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
